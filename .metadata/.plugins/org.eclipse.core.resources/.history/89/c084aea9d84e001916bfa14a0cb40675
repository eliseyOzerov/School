#include "Calendar.h"
#include "Event.h"
#include <iostream>
#include <limits>

using namespace std;

int main(){

	Calendar* cal = new Calendar("Elisey");

	cal->addEvent(new Event("Programiranje1", "F-104",
			*new DateTime(*new Date(25, 3, 2019), *new Time(7, 0, 0)),
			*new DateTime(*new Date(25, 3, 2019), *new Time(10, 0, 0))));

	cout << cal->toString();
	return 0;
}


