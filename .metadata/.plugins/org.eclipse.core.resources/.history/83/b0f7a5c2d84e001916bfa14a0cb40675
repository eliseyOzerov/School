#include "Calendar.h"
#include "Event.h"
#include <iostream>
#include <limits>

using namespace std;

int main(){

	Calendar* cal = new Calendar("Elisey");
	Date * startDate = new Date(25, 3, 2019);
	Time * startTime = new Time(7, 0, 0);
	Date * endDate = new Date(25,3,2019);
	Time * endTime = new Time(10,0,0);
	DateTime * startDT = new DateTime(*startDate, *endDate);
	DateTime * endDT = new DateTime(*startTime, *endTime);

	cal->addEvent(new Event("Programiranje1", "F-104", *startDT, *endDT);

	cout << cal->toString();
	return 0;
}


