#include "DateTime.h"
#include <string>

class Event{
private:
	std::string name;
	std::string location;
	DateTime startTime;
	DateTime endTime;
public:
	Event(std::string name, std::string location,
			DateTime startTime, DateTime endTime);
	std::string toString();
};


